<p id="notice"><%= notice %></p>

<h1>Listing User Teams</h1>

<table>
  <thead>
    <tr>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_teams.each do |user_team| %>
      <tr>
        <td><%= user_team.role %></td>
        <td><%= link_to 'Show', user_team %></td>
        <td><%= link_to 'Edit', edit_user_team_path(user_team) %></td>
        <td><%= link_to 'Destroy', user_team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User team', new_user_team_path %>
